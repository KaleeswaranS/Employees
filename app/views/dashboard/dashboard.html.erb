      <div class="row" style="width: 245; margin-left: -18px;">
        <div class="col-lg-8" style="width: 783; height: 400;">
          <div class="card">
      <div id="filter_div">
        <label class="google-visualization-controls-label">Employee Project</label>
      </div>
      <div id="chart_div"></div>
    </div>
  </div>
